<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<h1><%= title %></h1>
<p>Welcome to <%= title %>
<h2>CSV Table</h2>
<% if (features) { %>
    <table>
        <tr>
            <th>#</th>
            <th>shortname_street</th>
            <th>formalname_street</th>
            <th>house_number</th>
            <th>building</th>
            <th>block</th>
            <th>letter</th>
            <th>built_year</th>
            <th>exploitation_start_year</th>
        </tr>
        <% features.forEach(function (feature, i) {
        if (i < 300){ %>
            <tr>
                <td><%= i %></td>
                <td><%= feature.shortname_street %></td>
                <td><%= feature.formalname_street %></td>
                <td><%= feature.house_number %></td>
                <td><%= feature.building %></td>
                <td><%= feature.block %></td>
                <td><%= feature.letter %></td>
                <td><%= feature.built_year %></td>
                <td><%= feature.exploitation_start_year %></td>
            </tr>
        <% }
        }) %>
    </table>
<% } %>
</body>
</html>
